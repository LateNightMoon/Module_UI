<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GridTextView">
        <attr name="maxLength" format="integer" />
        <attr name="strokeRadius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeColor" format="color" />
        <attr name="pointColor" format="color" />
        <attr name="pointRadius" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textType">
            <enum name="text" value="1" />
            <enum name="point" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="NumberKeyboard">
            <attr name="deleteKeySrc" format="reference" />
            <attr name="deleteKeyWidth" format="dimension|reference" />
            <attr name="deleteKeyHeight" format="dimension|reference" />>
            <attr name="deleteKeyBackgroundColor" format="color|reference" />
            <attr name="emptyKeyBackgroundColor" format="color|reference" />
    </declare-styleable>
</resources>